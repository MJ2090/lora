f", "answer": "\u4e0d\u540c\u51fa\u884c\u8005\u5bf9\u540c\u4e00\u8def\u6bb5\u7684\u4f30\u8ba1\u503c\u4e0d\u4f1a\u5b8c\u5168\u76f8\u540c\u3002"}, {"question": "\u8c01\u63d0\u51fa\u4e86\u968f\u673a\u6027\u5206\u914d\u7684\u7406\u8bba\uff1f", "answer": "\u9ebb\u7701\u7406\u5de5\u5b66\u9662\u7684Sheffi\u6559\u6388\u53ca\u7f8e\u56fd\u52a0\u5dde\u5927\u5b66\u4f2f\u514b\u5229\u5206\u6821\u7684Daganzo\u6559\u6388\u63d0\u51fa\u4e86\u968f\u673a\u6027\u5206\u914d\u7684\u7406\u8bba\u3002"}, {"question": "\u968f\u673a\u6027\u5206\u914d\u7684\u7406\u8bba\u7684\u524d\u63d0\u662f\u4ec0\u4e48\uff1f", "answer": "\u968f\u673a\u6027\u5206\u914d\u7684\u7406\u8bba\u7684\u524d\u63d0\u662f\u8ba4\u4e3a\u51fa\u884c\u8005\u5bf9\u8def\u6bb5\u963b\u6297\u7684\u4f30\u8ba1\u503c\u4e0e\u5b9e\u9645\u503c\u4e4b\u95f4\u7684\u5dee\u522b\u662f\u4e00\u4e2a\u968f\u673a\u53d8\u91cf\uff0c\u51fa\u884c\u8005\u4f1a\u5728\u201c\u591a\u6761\u5f84\u8def\u201d\u4e2d\u9009\u62e9\uff0c\u540c\u4e00\u8d77\u8fc4\u70b9\u7684\u6d41\u91cf\u4f1a\u901a\u8fc7\u4e0d\u540c\u7684\u5f84\u8def\u5230\u8fbe\u76ee\u7684\u5730\u3002"}, {"question": "\u968f\u673a\u6027\u5206\u914d\u7684\u7406\u8bba\u7684\u4f5c\u7528\u662f\u4ec0\u4e48\uff1f", "answer": "\u968f\u673a\u6027\u5206\u914d\u7684\u7406\u8bba\u7684\u4f5c\u7528\u662f\u5728\u62df\u5408\u3001\u53cd\u6620\u73b0\u5b9e\u4ea4\u901a\u7f51\u7edc\u5b9e\u9645\u7684\u8fdb\u7a0b\u4e2d\u63a8\u8fdb\u4e86\u4e00\u5927\u6b65\u3002"}, {"question": "\u968f\u673a\u6027\u5206\u914d\u7684\u7406\u8bba\u662f\u54ea\u4e00\u5e74\u63d0\u51fa\u7684\uff1f", "answer": "\u968f\u673a\u6027\u5206\u914d\u7684\u7406\u8bba\u662f\u57281977\u5e74\u63d0\u51fa\u7684\u3002"}, {"question": "\u4ec0\u4e48\u662f\u4ea4\u901a\u6d41\u5206\u914d\u7406\u8bba\uff1f", "answer": "\u4ea4\u901a\u6d41\u5206\u914d\u7406\u8bba\u662f\u7814\u7a76\u8def\u7f51\u4e0a\u4ea4\u901a\u6d41\u7684\u62e5\u6324\u6027\u3001\u8def\u5f84\u9009\u62e9\u7684\u968f\u673a\u6027\u3001\u4ea4\u901a\u9700\u6c42\u7684\u52a8\u6001\u6027\u5e76\u63a2\u6c42\u65b0\u7684\u4ea4\u901a\u6d41\u5206\u914d\u65b9\u6cd5\u7684\u7406\u8bba\u3002"}, {"question": "\u4ea4\u901a\u6d41\u5206\u914d\u7406\u8bba\u4e2d\u5b58\u5728\u54ea\u4e9b\u4ea4\u4e92\u4f5c\u7528\uff1f", "answer": "\u4ea4\u901a\u6d41\u5206\u914d\u7406\u8bba\u4e2d\u5b58\u5728\u8def\u7f51\u4e0a\u7684\u62e5\u6324\u6027\u3001\u8def\u5f84\u9009\u62e9\u7684\u968f\u673a\u6027\u3001\u4ea4\u901a\u9700\u6c42\u7684\u52a8\u6001\u6027\u4e09\u79cd\u4ea4\u4e92\u4f5c\u7528\u3002"}, {"question": "\u4ea4\u901a\u6d41\u5206\u914d\u7406\u8bba\u7684\u76ee\u7684\u662f\u4ec0\u4e48\uff1f", "answer": "\u4ea4\u901a\u6d41\u5206\u914d\u7406\u8bba\u7684\u76ee\u7684\u662f\u5c06\u8def\u7f51\u4e0a\u4ea4\u901a\u6d41\u7684\u62e5\u6324\u6027\u3001\u8def\u5f84\u9009\u62e9\u7684\u968f\u673a\u6027\u3001\u4ea4\u901a\u9700\u6c42\u7684\u65f6\u53d8\u6027\u7efc\u5408\u96c6\u6210\u53cd\u6620\u51fa\u6765\uff0c\u5e76\u63a2\u6c42\u65b0\u7684\u4ea4\u901a\u6d41\u5206\u914d\u65b9\u6cd5\u3002"}, {"question": "\u672a\u6765\u4ea4\u901a\u6d41\u5206\u914d\u7406\u8bba\u7684\u53d1\u5c55\u65b9\u5411\u662f\u4ec0\u4e48\uff1f", "answer": "\u6587\u672c\u4e2d\u672a\u63d0\u53ca\u672a\u6765\u4ea4\u901a\u6d41\u5206\u914d\u7406\u8bba\u7684\u53d1\u5c55\u65b9\u5411\u3002"}, {"question": "\u4e3b\u8981\u4ecb\u7ecd\u4e86\u54ea\u4e9b\u5185\u5bb9\uff1f", "answer": "\u4e3b\u8981\u4ecb\u7ecd\u4e86\u4ea4\u901a\u6d41\u5206\u914d\u3001\u4ea4\u901a\u963b\u6297\u3001\u5f84\u8def\u548c\u6700\u77ed\u5f84\u8def\u3001\u4ea4\u901a\u5e73\u8861\u95ee\u9898\u4ee5\u53ca\u4ea4\u901a\u5c0f\u533a\u4e0e\u4ea4\u901a\u7f51\u7edc\u7684\u5bf9\u5e94\u3002"}, {"question": "\u4ec0\u4e48\u662f\u4ea4\u901a\u6d41\u5206\u914d\uff1f", "answer": "\u4ea4\u901a\u6d41\u5206\u914d\u662f\u6307\u5c06\u4ea4\u901a\u9700\u6c42\u5206\u914d\u5230\u4ea4\u901a\u7f51\u7edc\u4e2d\u7684\u5404\u4e2a\u8def\u5f84\u4e0a\u7684\u8fc7\u7a0b\u3002"}, {"question": "\u4ec0\u4e48\u662f\u4ea4\u901a\u963b\u6297\uff1f", "answer": "\u4ea4\u901a\u963b\u6297\u662f\u6307\u5728\u4ea4\u901a\u7f51\u7edc\u4e2d\uff0c\u8def\u5f84\u4e0a\u7684\u4ea4\u901a\u963b\u529b\u4e0e\u8def\u5f84\u957f\u5ea6\u7684\u6bd4\u503c\u3002"}, {"question": "\u4ec0\u4e48\u662f\u4ea4\u901a\u5e73\u8861\u95ee\u9898\uff1f", "answer": "\u4ea4\u901a\u5e73\u8861\u95ee\u9898\u662f\u6307\u5728\u4ea4\u901a\u7f51\u7edc\u4e2d\uff0c\u4ea4\u901a\u9700\u6c42\u4e0e\u4ea4\u901a\u4f9b\u7ed9\u4e4b\u95f4\u8fbe\u5230\u4e00\u79cd\u5e73\u8861\u72b6\u6001\u7684\u95ee\u9898\u3002"}, {"question": "\u4ec0\u4e48\u662f\u4ea4\u901a\u6d41\u5206\u914d\uff1f", "answer": "\u4ea4\u901a\u6d41\u5206\u914d\u662f\u5c06\u9884\u6d4b\u5f97\u51fa\u7684 OD\u4ea4\u901a\u91cf\uff0c\u6839\u636e\u5df2\u77e5\u7684\u9053\u8def\u7f51\u63cf\u8ff0\uff0c\u6309\u7167\u4e00\u5b9a\u7684\u89c4\u5219\u7b26\u5408\u5b9e\u9645\u5730\u5206\u914d\u5230\u8def\u7f51\u4e2d\u7684\u5404\u6761\u9053\u8def\u4e0a\u53bb\uff0c\u8fdb\u800c\u6c42\u51fa\u8def\u7f51\u4e2d\u5404\u8def\u6bb5\u7684\u4ea4\u901a\u6d41\u91cf\u3001\u6240\u4ea7\u751f\u7684OD\u8d39\u7528\u77e9\u9635\uff0c\u5e76\u636e\u6b64\u5bf9\u57ce\u5e02\u4ea4\u901a\u7f51\u7edc\u7684\u4f7f\u7528\u72b6\u51b5\u505a\u51fa\u5206\u6790\u548c\u8bc4\u4ef7\u3002"}, {"question": "\u4ea4\u901a\u6d41\u5206\u914d\u7684\u76ee\u7684\u662f\u4ec0\u4e48\uff1f", "answer": "\u4ea4\u901a\u6d41\u5206\u914d\u7684\u76ee\u7684\u662f\u5bf9\u57ce\u5e02\u4ea4\u901a\u7f51\u7edc\u7684\u4f7f\u7528\u72b6\u51b5\u505a\u51fa\u5206\u6790\u548c\u8bc4\u4ef7\u3002"}, {"question": "\u4ea4\u901a\u6d41\u5206\u914d\u7684\u4f9d\u636e\u662f\u4ec0\u4e48\uff1f", "answer": "\u4ea4\u901a\u6d41\u5206\u914d\u7684\u4f9d\u636e\u662f\u5df2\u77e5\u7684\u9053\u8def\u7f51\u63cf\u8ff0\u548c\u9884\u6d4b\u5f97\u51fa\u7684OD\u4ea4\u901a\u91cf\u3002"}, {"question": "\u4ea4\u901a\u6d41\u5206\u914d\u7684\u7ed3\u679c\u5305\u62ec\u54ea\u4e9b\u5185\u5bb9\uff1f", "answer": "\u4ea4\u901a\u6d41\u5206\u914d\u7684\u7ed3\u679c\u5305\u62ec\u8def\u7f51\u4e2d\u5404\u8def\u6bb5\u7684\u4ea4\u901a\u6d41\u91cf\u548c\u6240\u4ea7\u751f\u7684OD\u8d39\u7528\u77e9\u9635\u3002"}, {"question": "\u4ec0\u4e4